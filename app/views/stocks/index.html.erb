<%= render "partials/navbar" %>

<% if @user_type == 'broker' %>
  <section>
      <div class="container">
      <h1>Stocks</h1>
        <%= render "broker/stocks", stocks_list: @stocks_list %>
      </div>
  </section>

<% elsif @user_type == 'buyer' %>
  <section>
    <div class="container">
      <h1>Stocks</h1>
      <%= render "buyer/stocks", broker_stocks_list: @broker_stocks_list %>
    </div>
  </section>
  
<% end %>
